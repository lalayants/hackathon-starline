<?xml version='1.0'?>
<sdf version='1.7'>
  <model name='bereg'>
    <pose>0 0 0 0 0 0</pose>
    
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>1.14395</mass>
        <inertia>
          <ixx>0.126164</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.416519</iyy>
          <iyz>0</iyz>
          <izz>0.481014</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <pose>0 0 0.3 0 0 1.5707</pose>
        <geometry>
          <mesh>
            <uri>model://bereg/body.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <transparency>0</transparency>
        <cast_shadows>1</cast_shadows>
      </visual>
      <collision name='collision'>
        <pose>0 0 1.18 0 0 0.785398</pose>
        <geometry>
          <box>
            <size>0.6 0.6 1.76</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name='left_wheel'>
      <pose>0 0.55 0.1 -1.5707 0 0</pose>
      <inertial>
        <mass>2</mass>
        <inertia>
          <ixx>0.145833</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.145833</iyy>
          <iyz>0</iyz>
          <izz>0.125</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://bereg/wheel.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name='collision'>
        <geometry>
          <sphere>
            <radius>0.2</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
              <max_vel>0.01</max_vel>
              <min_depth>0.01</min_depth>
            </ode>
          </contact>
        </surface>
      </collision>
    </link>

    <link name='right_wheel'>
      <pose>0 -0.55 0.1 -1.5707 0 0</pose>
      <inertial>
        <mass>2</mass>
        <inertia>
          <ixx>0.145833</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.145833</iyy>
          <iyz>0</iyz>
          <izz>0.125</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://bereg/wheel.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name='collision'>
        <geometry>
          <sphere>
            <radius>0.2</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
              <max_vel>0.01</max_vel>
              <min_depth>0.01</min_depth>
            </ode>
          </contact>
        </surface>
      </collision>
    </link>

    <joint name='left_joint' type='revolute'>
      <parent>base_link</parent>
      <child>left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>

    <joint name='right_joint' type='revolute'>
      <parent>base_link</parent>
      <child>right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
      </axis>
    </joint>


    <link name='caster_front'>
      <pose>0.55 0 0.105 0 0 -1.5707</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://bereg/wheel.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name='collision'>
        <geometry>
          <sphere>
            <radius>0.2</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <link name='caster_rear'>
      <pose>-0.55 0 0.105 0 0 -1.5707</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.1</iyy>
          <iyz>0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://bereg/wheel.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name='collision'>
        <geometry>
          <sphere>
            <radius>0.2</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <joint name='caster_front_joint' type='ball'>
      <parent>base_link</parent>
      <child>caster_front</child>
    </joint>

    <joint name='caster_rear_joint' type='ball'>
      <parent>base_link</parent>
      <child>caster_rear</child>
    </joint>


    <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>

      <!-- <ros>
        <namespace>/demo</namespace>
        <remapping>cmd_vel:=cmd_demo</remapping>
        <remapping>odom:=odom_demo</remapping>
      </ros> -->

      <!-- Number of wheel pairs -->
      <!-- <num_wheel_pairs>2</num_wheel_pairs> -->

      <!-- wheels0 -->
      <left_joint>left_joint</left_joint>
      <right_joint>right_joint</right_joint>

      <!-- kinematics -->
      <wheel_separation>1.1</wheel_separation>
      <!-- <wheel_separation>1.4</wheel_separation> -->

      <wheel_diameter>0.4</wheel_diameter>
      <!-- <wheel_diameter>0.5</wheel_diameter> -->

      <!-- limits -->
      <max_wheel_torque>10</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>

      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

    </plugin>


     <link name="lidar">
       <pose>0 0 0.775 0 0 0</pose>
       <visual name='visual'>
         <geometry>
           <sphere>
             <radius>0.02</radius>
           </sphere>
         </geometry>
         <material>
           <ambient>0.1 0.1 0.1 1</ambient>
           <diffuse>0.1 0.1 0.2 1</diffuse>
         </material>
       </visual>

       <sensor name="lidar_sensor" type="ray">
         <always_on>true</always_on>
         <update_rate>10.0</update_rate>
         <visualize>false</visualize>
         <ray>
           <scan>
             <horizontal>
               <samples>300</samples>
               <resolution>1.0</resolution>
               <min_angle>-3.14159265</min_angle>
               <max_angle>3.14159265</max_angle>
             </horizontal>
           </scan>
           <range>
             <min>0.5</min>
             <max>8.0</max>
           </range>
           <noise>
             <type>gaussian</type>
             <mean>0.0</mean>
             <stddev>0.01</stddev>
           </noise>
         </ray>

         <plugin name="lidar_plugin" filename="libgazebo_ros_ray_sensor.so">
           <ros>
             <remapping>~/out:=scan</remapping>
           </ros>

           <!-- <frame_name>lidar</frame_name> -->
           <output_type>sensor_msgs/LaserScan</output_type>
         </plugin>

       </sensor>

     </link>

     <joint name="lidar_joint" type="fixed">
       <parent>base_link</parent>
       <child>lidar</child>
     </joint>


     <link name="camera">
       <pose>0.42 0 1.75 0 0 0</pose>
       <visual name='visual'>
         <geometry>
           <sphere>
             <radius>0.04</radius>
         </sphere>
          </geometry>
          <material>
            <ambient>0.1 0.1 0.1 1</ambient>
            <diffuse>0.1 0.1 0.2 1</diffuse>
          </material>
       </visual>

       <sensor type="depth" name="camera">
         <update_rate>30</update_rate>
         <visualize>false</visualize>
         <camera>
           <horizontal_fov>1.57</horizontal_fov>
           <image>
             <width>640</width>
             <height>480</height>
             <format>B8G8R8</format>
           </image>
           <depth_camera>
           </depth_camera>
           <clip>
             <near>0.1</near>
             <far>300</far>
           </clip>
           <noise>
             <type>gaussian</type>
             <mean>0.0</mean>
             <stddev>0.007</stddev>
           </noise>
         </camera>

         <plugin name="camera_plugin" filename="libgazebo_ros_camera.so">

           <camera_name>camera</camera_name>
           <frame_name>camera</frame_name>

           <min_depth>0.4</min_depth>
           <max_depth>16</max_depth>

         </plugin>

       </sensor>

     </link>

     <joint name="camera_joint" type="fixed">
       <parent>base_link</parent>
       <child>camera</child>
     </joint>

  </model>
</sdf>
